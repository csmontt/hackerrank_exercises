   "source": [
    "def findDigits(n):\n",
    "    n_as_string = str(n)\n",
    "    count = 0\n",
    "    for s in n_as_string:\n",
    "        if int(s) != 0 and (n % int(s)) == 0:\n",
    "            count += 1\n",
    "    return(count)"
   ]
